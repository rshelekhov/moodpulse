{
	"name": "moodpulse",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "bun run src/index.ts",
		"dev": "bun --watch run src/index.ts",
		"test": "bun test",
		"lint": "bunx --bun biome check .",
		"lint:fix": "bunx --bun biome check --write .",
		"format": "bunx --bun biome format --write .",
		"prisma:generate": "bunx --bun prisma generate",
		"prisma:migrate": "bunx --bun prisma migrate dev",
		"prisma:migrate:prod": "bunx --bun prisma migrate deploy --schema src/infrastructure/prisma/schema.prisma",
		"prisma:studio": "bunx --bun prisma studio"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@biomejs/cli-darwin-x64": "^2.3.14",
		"@types/bun": "latest",
		"@types/papaparse": "^5.5.2",
		"pino-pretty": "^13.1.3",
		"prisma": "^7.3.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@date-fns/tz": "^1.4.1",
		"@grammyjs/conversations": "^2.1.1",
		"@napi-rs/canvas": "^0.1.90",
		"@prisma/adapter-pg": "^7.3.0",
		"@prisma/client": "^7.3.0",
		"chart.js": "^4.5.1",
		"croner": "^10.0.1",
		"form-data": "^4.0.5",
		"grammy": "^1.39.3",
		"mailgun.js": "^12.7.0",
		"papaparse": "^5.5.3",
		"pino": "^10.3.0",
		"xlsx": "^0.18.5",
		"zod": "^4.3.6"
	}
}
